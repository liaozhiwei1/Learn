<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MyBatis.testMapper">

    <resultMap id="BaseResultMap" type="MyBatis.dto">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="template_name" property="templateName" jdbcType="BIGINT" />
        <result column="type" property="type" jdbcType="BIGINT" />
        <result column="create_by" property="createBy" jdbcType="BIGINT" />
        <result column="update_by" property="updateBy" jdbcType="BIGINT" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
    </resultMap>

    <select id="find" parameterType="java.lang.String" resultMap="BaseResultMap">
         SELECT
            t1.id,
            t1.template_name,
            t1.type,
            t1.create_by,
            t1.update_by,
            t1.update_time,
            t1.create_time
        FROM
            image_process_template t1
        where t1.template_name = #{templateName,jdbcType=VARCHAR}

    </select>
</mapper>